{
  "jade": {
    "command": [
      "jade -Po ../ #{$fileName}.jade"
    ]
  },
  "ls": {
    "command": [
      "lsc -cp #{$fileName.ls}>../js/#{$fileName}.js"
    ]
  },
  "scss": {
    "command": [
      "sass --sourcemap=none --style compact #{$fileName}.scss ../css/#{$fileName}.css"
    ]
  }
}
